partial alphanumeric_keys modifier_keys 
xkb_symbols "eurkey-cmk-dh-iso-wide-custom"  {
        include "eu(eurkey-cmk-dh-ansi)"
        name[Group1] = "EurKEY (Colemak-DH, ISO)";

        key <LSGT> { [              z,                 Z,               yacute,               Yacute ] };

        key <AB05>	{[    backslash,             bar,              notsign,            brokenbar  ]};

        key <AB06> { [     numbersign,        dead_tilde,              section,                tilde ] }; //change AB05 to LSGT

        key <BKSL> { [      Return,        Return,          Return,           Return ] };
};
